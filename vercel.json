{
  "version": 2,
  "builds": [
    { "src": "public/index.html", "use": "@vercel/static" },
    { "src": "api/socket.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/socket.io/(.*)", "dest": "/api/socket" },
    { "src": "/api/socket", "dest": "/api/socket" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
